{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "project.de-mobile-app",
  "projectType": "application",
  "sourceRoot": "apps/project.de/mobile-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/expo:build",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "download": {
      "executor": "@nx/expo:download",
      "options": {
        "output": "apps/project.de/mobile-app/dist"
      }
    },
    "ensure-symlink": {
      "executor": "@nx/expo:ensure-symlink",
      "options": {}
    },
    "export": {
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "executor": "@nx/expo:export",
      "options": {
        "outputDir": "../../../dist/apps/project.de/mobile-app",
        "platform": "all"
      }
    },
    "export-web": {
      "executor": "@nx/expo:export",
      "options": {
        "bundler": "metro"
      }
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/project.de/mobile-app/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "prebuild": {
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "executor": "@nx/expo:prebuild",
      "options": {}
    },
    "run-android": {
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      }
    },
    "run-ios": {
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx start mobile-app"
      }
    },
    "start": {
      "dependsOn": ["ensure-symlink", "sync-deps"],
      "executor": "@nx/expo:start",
      "options": {
        "port": 8081
      }
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/project.de/mobile-app/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    }
  }
}
