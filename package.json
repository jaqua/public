    "serve:editor": "nx serve project.de-editor",
    "serve:main": "nx serve project.de-main",
    "serve:mobile-app": "nx start project.de-mobile-app",
    "serve:mobile-app-expo": "cd apps/project.de/mobile-app && npx expo start",
    "dev": "npm-run-all -l -p serve:api serve:editor serve:main",
    "typing": "npx graphql-codegen --config tools/scripts/typings/codegen.ts project.de",
    "typing:shared": "npx graphql-codegen --config tools/scripts/typings/codegen.ts shared"
