    "test": "npm run affected:lint && npm run affected:test && npm run test:merge-reports && npm run affected:e2e",
    "test:all": "nx run-many --all --target=test --parallel=5 --codeCoverage",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --all --target=lint --parallel=5",
    "e2e-api:project.de": "start-server-and-test \"NODE_ENV=test nx run project.de-api:serve\" 3040",
